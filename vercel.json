{
    "version": 2,
    "builds": [
        {
            "src": "frontend/view/**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "/frontend/view/index.html"
        },
        {
            "src": "/index.js",
            "dest": "/frontend/view/index.js"
        },


        {
            "src": "/site/inicio.html",
            "dest": "/frontend/view/site/inicio.html"
        }
    ]
}