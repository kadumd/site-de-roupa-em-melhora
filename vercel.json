{
    "version": 2,
    "builds": [
        {
            "src": "frontend/view/**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "/frontend/view/pasta-do-site/inicio/inicio.html"
        },
        {
            "src": "/frontend/view/pasta-do-site/inicio/main.css",
            "dest": "/frontend/view/pasta-do-site/inicio/main.css"
        }
    ]
}